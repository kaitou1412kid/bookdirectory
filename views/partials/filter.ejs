
<table class="table table-striped table-hover table-bordered table-sm">
    <tr>
        <th class="sn">S.N</th>
        <th>Title</th>
        <th>ISBN 10</th>
        <th>Author</th>
    </tr>
    <% var n=1 %> 
    <% newBookItem.forEach(function(book){ %>
    <tr>
        <td class="sn"><%= n %><% n++ %></td>
        <td><%= book.title %></td>
        <td><%= book.isbn %> </td>
        <td><%= book.author %> </td>
    </tr>   
    <% }) %>
</table>